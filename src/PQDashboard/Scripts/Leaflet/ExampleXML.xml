<?xml version="1.0" encoding="utf-8" ?>
<Layers>
	<Layer>
		<!-- ID is for the layer ID on the URI -->
		<ID>0</ID>
		<!-- Name is for the name of the layer on PQ Dashboard for map control -->
		<Name>Example 1</Name>
		<!-- This defines the image used, by default the path is relative to /PQDashboard (Only used for points)-->
		<Image>/Scripts/Leaflet/images/Example.png</Image>
	</Layer>
	<Layer>
		<ID>1</ID>
		<Name>Example 2</Name>
		<!-- This defines the color of this layer, uses hex code (Only used for polylines)-->
		<Color>#eb4034</Color>
	</Layer>
	<!-- Neither property must be specified for the layer to display, it will default on the icon and color.-->
	<Layer>
		<ID>1</ID>
		<Name>Example 2</Name>
	</Layer>
</Layers>