    <script src="@("~/@GSF/Web/Shared/Scripts/react.production.min.js")"></script>
    <script src="@("~/@GSF/Web/Shared/Scripts/react-dom.production.min.js")"></script>
}
    <script type="text/javascript" src="~/Scripts/PQDashboard.js"></script>

</body>
</html>