                            {
                                <div id="OverviewDisturbancesMagDur" class="docklet chart-container" style="display: none"></div>
                            }
                        </div>
                    </div>
                    <div class="portlet" id="Portlet2@(tab)">
                        <div class="portlet-header">
                            <div class="portlet-header-text" style="display: inline">
